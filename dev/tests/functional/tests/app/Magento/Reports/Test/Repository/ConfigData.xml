<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Magento\Config\Test\Repository\ConfigData">
        <dataset name="compare_products">
            <field name="catalog/recently_products/scope" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string">Website</item>
                <item name="value" xsi:type="string">Website</item>
            </field>
            <field name="catalog/recently_products/viewed_count" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string"/>
                <item name="value" xsi:type="number">5</item>
            </field>
            <field name="catalog/recently_products/compared_count" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string"/>
                <item name="value" xsi:type="number">12</item>
            </field>
        </dataset>

        <dataset name="website_with_custom_timezone">
            <field name="general/locale/timezone" xsi:type="array">
                <item name="label" xsi:type="string">Timezone</item>
                <item name="value" xsi:type="string">Australia/Sydney</item>
            </field>
            <field name="scope" xsi:type="array">
                <item name="scope_type" xsi:type="string">store</item>
                <item name="set_level" xsi:type="string">website</item>
                <item name="dataset" xsi:type="string">custom_store</item>
            </field>
        </dataset>

        <dataset name="notify_stock_quantity_2">
            <field name="cataloginventory/item_options/notify_stock_qty" xsi:type="array">
                <item name="scope" xsi:type="string">catalog</item>
                <item name="scope_id" xsi:type="number">1</item>
                <item name="label" xsi:type="string"/>
                <item name="value" xsi:type="number">2</item>
            </field>
        </dataset>
    </repository>
</config>
