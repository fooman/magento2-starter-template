<mapping strict="0">
<fields>
    <customer composite="1">
        <firstname>
            <selector>input[name*=firstname]</selector>
        </firstname>
        <lastname>
            <selector>input[name*=lastname]</selector>
        </lastname>
        <email>
            <selector>input[id=email]</selector>
        </email>
        <current_password>
            <selector>input[name*=current-password]</selector>
        </current_password>
        <password>
            <selector>input[name*=current-password]</selector>
        </password>
    </customer>
    <change_password>
        <input>checkbox</input>
    </change_password>
    <change_email>
        <input>checkbox</input>
    </change_email>
    <confirmation />
</fields>
</mapping>
